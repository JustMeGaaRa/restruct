{
    "name": "@restruct/cli",
    "version": "0.0.0",
    "type": "module",
    "bin": {
        "restruct": "./dist/index.js"
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm --clean --dts && pnpm run build:preview",
        "build:preview": "vite build -c preview-app/vite.config.ts",
        "dev": "tsup src/index.ts --format esm --watch --clean --dts",
        "start": "node dist/index.js",
        "lint": "eslint .",
        "test": "vitest"
    },
    "dependencies": {
        "@chakra-ui/react": "^3.33.0",
        "@emotion/react": "^11.14.0",
        "@graph/svg": "workspace:*",
        "@structurizr/dsl": "workspace:*",
        "@structurizr/react": "workspace:*",
        "@restruct/ui": "workspace:*",
        "chalk": "^5.3.0",
        "chokidar": "^3.6.0",
        "commander": "^12.0.0",
        "esbuild": "^0.20.0",
        "fs-extra": "^11.2.0",
        "inquirer": "^9.2.14",
        "ora": "^8.0.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "vite": "^5.1.4",
        "ws": "^8.16.0"
    },
    "devDependencies": {
        "@restruct/eslint-config": "workspace:*",
        "@restruct/typescript-config": "workspace:*",
        "@types/fs-extra": "^11.0.4",
        "@types/inquirer": "^9.0.7",
        "@types/node": "^20.11.24",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "@types/ws": "^8.5.10",
        "@vitejs/plugin-react": "^4.2.1",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3",
        "vite-tsconfig-paths": "^6.1.1",
        "vitest": "^1.3.1"
    }
}
